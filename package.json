{
  "name": "multi-discord-api",
  "version": "0.0.0-development",
  "description": "Multi-API for Nodejs Projects and with no rate-limit using JavaScript &lt;3",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run lint && babel ./src -d ./lib",
    "test": "nyc mocha --require babel-core/register",
    "lint": "eslint ./src",
    "commit": "git-cz",
    "semantic-release": "semantic-release --branches main"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SidisLiveYT/multi-discord-api.git"
  },
  "keywords": [
    "api",
    "discord",
    "multi-api",
    "discord api",
    "multi-discord-api"
  ],
  "author": "Sid is Live YT",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SidisLiveYT/multi-discord-api/issues"
  },
  "homepage": "https://github.com/SidisLiveYT/multi-discord-api#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0",
    "semantic-release": "^17.4.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
